{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_",
        "exclude": [
            "node_modules",
            "plugins"
        ]
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "systemName"      : "DocStrap",
        "footer"          : "",
        "copyright"       : "Copyright Â© 2016 The contributors to Music Compiler projects.",
        "navType"         : "vertical",
        "theme"           : "cerulean",
        "linenums"        : false,
        "collapseSymbols" : false,
        "inverseNav"      : true
    }
}
